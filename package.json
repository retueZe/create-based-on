{
  "private": true,
  "name": "create-by-template",
  "version": "1.0.0-a.1",
  "author": "retueze",
  "license": "MIT",
  "description": "Build tool based on custom templates.",
  "keywords": [
    "package",
    "project",
    "factory",
    "initializer",
    "build",
    "template",
    "configure"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/retueZe/create-by-template"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./package.json": "./package.json"
  },
  "scripts": {
    "lint": "eslint .",
    "build:prod": "npm run lint && rollup -c",
    "build:dev": "tsc --outDir dist --inlineSourceMap --module commonjs",
    "clean": "rm -rf dist",
    "prepare": "npm run clean && npm run build:prod",
    "postpublish": "npm run clean"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.1",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/node": "^20.2.5",
    "@types/prompts": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "rollup": "^3.21.2",
    "typescript": "^5.0.4"
  }
}
